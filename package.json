{
  "name": "redux-detector",
  "version": "0.5.0",
  "description": "Redux extension allows to detect state changes and then dispatch actions using pure functions.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "build:umd": "cross-env NODE_ENV=development webpack lib/index.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack lib/index.min.js",
    "build": "npm run clean && npm run build:umd && npm run build:umd:min",
    "prepublish": "npm run build && npm run test",
    "test": "NODE_ENV=test karma start"
  },
  "repository": {
    "url": "https://github.com/piotr-oles/redux-detector.git",
    "type": "git"
  },
  "keywords": [
    "redux",
    "reducer",
    "state",
    "predictable",
    "functional",
    "immutable",
    "detector",
    "change",
    "flux",
    "elm"
  ],
  "author": "Piotr Ole≈õ <piotrek.oles@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/piotr-oles/redux-detector/issues"
  },
  "dependencies": {
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "cross-env": "^3.1.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^2.1.7",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "ts-loader": "^2.0.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.4",
    "webpack": "2.2.0-rc.0"
  },
  "peerDependencies": {
    "redux": "^3.1.0"
  }
}
